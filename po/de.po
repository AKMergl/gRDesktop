#: ../src/optbox.c:1147
msgid "Save rdp file"
msgstr "RDP-Datei speichern"

#: ../src/optbox.c:1150 ../src/optbox.c:1178 ../src/optbox.c:1435
msgid "_Cancel"
msgstr "_Abbruch"

#: ../src/optbox.c:1151
msgid "_Save"
msgstr "_Speichern"

#: ../src/optbox.c:1175
msgid "Open rdp file"
msgstr "RDP-Datei öffnen"

#: ../src/optbox.c:1179 ../src/optbox.c:1436
msgid "_Open"
msgstr "_Öffnen"

#: ../src/optbox.c:1432
msgid "Choose folder to share"
msgstr "Verzeichnis für Weitergabe wählen"

#: ../src/optbox.c:1461
msgid "Share must be defined!"
msgstr "Share soll definiert werden!"

#: ../src/optbox.c:1468
msgid "Path must be defined!"
msgstr "Pfad soll definiert werden!"

#: ../src/optbox.c:1472
msgid "It is not an absolute path!"
msgstr "Ein Absolutpfad soll angegeben werden!"

#: ../src/options.c:34
msgid "Windows NT/2000"
msgstr "Windows NT/2000"

#: ../src/options.c:35
msgid "Windows XP/2003"
msgstr "Windows XP/2003"

#: ../src/options.c:70 ../src/options.c:422 ../src/rdpparse.c:121
msgid "Fullscreen"
msgstr "Vollbild"

#: ../src/options.c:116
msgid "256 Colors"
msgstr "256 Farben"

#: ../src/options.c:117
msgid "High Color (15bit)"
msgstr "High Color (15bit)"

#: ../src/options.c:118
msgid "High Color (16bit)"
msgstr "High Color (16bit)"

#: ../src/options.c:119
msgid "True Color (24bit)"
msgstr "True Color (24bit)"

