msgid "Run program"
msgstr "Programm starten"

#: ../src/optbox.c:926
msgid "Path and filename:"
msgstr "Pfad und Dateiname:"

#: ../src/optbox.c:940
msgid "Working directory:"
msgstr "Arbeitsverzeichnis:"

#. insert checkboxes
#: ../src/optbox.c:965 ../src/grdesktop.schemas.in.h:1
msgid "Force bitmap updates"
msgstr "Bitmap-Updates erzwingen"

#: ../src/optbox.c:972
msgid "Don't send motion events"
msgstr "Motionevents nicht senden"

#: ../src/optbox.c:979
msgid "Hide window manager decorations"
msgstr "Fenstermanager-Dekoration verbergen"

#: ../src/optbox.c:986
msgid "Disable encryption (French TS)"
msgstr "Verschlüsselung deaktivieren (Französischer TS)"

#: ../src/optbox.c:993
msgid "Window manager key bindings"
msgstr "Fenstermanager-Tasten erlauben"

#: ../src/optbox.c:1000
msgid "Attach to console (>= Windows 2003)"
msgstr "Zur Konsole verbinden (>= Windows 2003)"

#: ../src/optbox.c:1021
msgid "Fake clientname:"
msgstr "Clientname fälschen"

#: ../src/optbox.c:1051 ../src/sshbox.c:66
msgid "Enable SSH tunneling"
msgstr "SSH-Tunnel aktivieren"

#: ../src/optbox.c:1057
msgid "Properties"
msgstr "Eigenschaften"

