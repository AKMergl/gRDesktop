#. create logon settings box
#: ../src/optbox.c:167
msgid "Logon Settings"
msgstr "Anmeldeoptionen"

#: ../src/optbox.c:201
msgid ""
"Type the name of the computer, or\n"
"choose a computer from the list."
msgstr ""
"Geben Sie den Namen des Computers ein,\n"
"oder wählen Sie einen Eintrag aus der Liste."

#. create open/save settings box
#: ../src/optbox.c:221
msgid "Connection settings"
msgstr "Gespeicherte Verbindungen"

#: ../src/optbox.c:252
msgid ""
"Save settings or load saved\n"
"connections from file."
msgstr ""
"Einstellungen speichern, oder gespeicherte\n"
"Einstellungen aus Datei Laden."

#. create display settings box
#: ../src/optbox.c:282
msgid "Remote desktop size"
msgstr "Entfernte Desktopgrösse"

#. create the top label
#: ../src/optbox.c:309
msgid ""
"Choose the size of your remote\n"
"desktop, or select fullscreen."
msgstr ""
"Wählen Sie die Grösse Ihres Remotedesktops\n"
"oder wählen Sie Vollbild."

#. create color settings box
#: ../src/optbox.c:322
msgid "Colors"
msgstr "Farben"

#. create the top label
#: ../src/optbox.c:349
msgid ""
"Select the available colorsize\n"
"on remote box."
msgstr ""
"Wählen Sie die Verfügbare Farbtiefe\n"
"der Remotebox aus."

#. create keyboard settings box
#: ../src/optbox.c:372
msgid "Keyboard type"
msgstr "Tastaturtyp"

#: ../src/optbox.c:403
msgid ""
"Select the language code for\n"
"the keyboard."
msgstr ""
"Wählen Sie den Ländercode für\n"
"die Tastatur aus."

#. create sound settings box
#: ../src/optbox.c:410
msgid "Sound on the remote Computer"
msgstr "Sound auf dem entfernten Rechner"

#: ../src/optbox.c:441
msgid ""
"Please select which sound output you\n"
"would like to use."
msgstr "Wählen Sie die Sound Ausgabeoption aus."

#. create program settings box
#: ../src/optbox.c:464
msgid "Launch Program"
msgstr "Programm starten"

#: ../src/optbox.c:495
msgid ""
"Select a program which should start\n"
"after connection."
msgstr ""
"Geben Sie ein Programm an, das nach dem\n"
"Verbindungsaufbau starten soll."

#. create program settings box
#: ../src/optbox.c:525
msgid "Options"
msgstr "Optionen"

#: ../src/optbox.c:556
msgid "Tune your remote desktop connections."
msgstr ""
"Optimieren Sie die Remotedesktop \n"
"Verbindung."

#. create frame Disk
#: ../src/optbox.c:603
msgid "Disk"
msgstr "Festplatte"

#: ../src/optbox.c:624
msgid "Select path to share on remote server."
msgstr ""
"Wählen Sie ein Verzeichnis,\n"
"um es am entfernten Rechner auch erreichen zu können."

#: ../src/optbox.c:638
msgid "Share name:"
msgstr "Share-Name:"

#: ../src/optbox.c:652
msgid "Local full path of share:"
msgstr "Lokaler Pfad der Weitergabe:"

#: ../src/optbox.c:672
msgid "Browse..."
msgstr "Suchen..."

#: ../src/optbox.c:678
msgid "Add"
msgstr "Übernehmen"

#. create entry for commandline optoins
#: ../src/optbox.c:685
msgid "Options to be added to the command"
msgstr "Kommandooptionen"

