#: ../src/options.c:214
msgid "No sound output"
msgstr "Keine Sound Ausgabe"

#: ../src/options.c:215
msgid "Play on this host"
msgstr "Diesen Computer verwenden"

#: ../src/rdpparse.c:53 ../src/rdpparse.c:81
msgid "Please select a valid rdp file!"
msgstr "Bitte wählen Sie eine gültige RDP-Datei aus!"

#: ../src/rdpparse.c:95
msgid "Really override the already existing file?"
msgstr "Solle die existierende Datei wirklich überschrieben werden?"

#: ../src/rdpparse.c:111
#, c-format
msgid "Unable to save rdp-file: %s"
msgstr "RDP-Datei konnte nicht gespeichert werden: %s"

#: ../src/rdpparse.c:187 ../src/rdpparse.c:196
#, c-format
msgid "Unable to open rdp-file: %s"
msgstr "RDP-Datei konnte nicht geöffnet werden: %s"

#: ../src/run.c:107
msgid "No hostname for ssh tunneling available"
msgstr "Kein Rechnername für den SSH-Tunnel angegeben."

#: ../src/sshbox.c:41
msgid "SSH-Tunnel settings"
msgstr "Verbindungseinstellungen"

#: ../src/sshbox.c:44
msgid "OK"
msgstr "OK"

#: ../src/sshbox.c:90
msgid "Hostname:"
