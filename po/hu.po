#. create logon settings box
#: ../src/optbox.c:167
msgid "Logon Settings"
msgstr "Bejelentkezés"

#: ../src/optbox.c:201
msgid ""
"Type the name of the computer, or\n"
"choose a computer from the list."
msgstr ""
"Írja be a számítógép nevét,\n"
"vagy válasszon a listából!"

#. create open/save settings box
#: ../src/optbox.c:221
msgid "Connection settings"
msgstr "Tárolt kapcsolatok"

#: ../src/optbox.c:252
msgid ""
"Save settings or load saved\n"
"connections from file."
msgstr ""
"Mentse el a beállításokat, vagy\n"
"töltsön be egy eltárolt kapcsolatot!"

#. create display settings box
#: ../src/optbox.c:282
msgid "Remote desktop size"
msgstr "A távoli asztal mérete"

#. create the top label
#: ../src/optbox.c:309
msgid ""
"Choose the size of your remote\n"
"desktop, or select fullscreen."
msgstr ""
"Válassza ki a távoli asztal méretét,\n"
"vagy a teljes képernyőt!"

#. create color settings box
#: ../src/optbox.c:322
msgid "Colors"
msgstr "Színek"

#. create the top label
#: ../src/optbox.c:349
msgid ""
"Select the available colorsize\n"
"on remote box."
msgstr ""
"Válassza ki a távoli gép \n"
"lehetséges színmélységét!"

#. create keyboard settings box
#: ../src/optbox.c:372
msgid "Keyboard type"
msgstr "Billentyűzet típusa"

#: ../src/optbox.c:403
msgid ""
"Select the language code for\n"
"the keyboard."
msgstr ""
"Válassza ki a billentyűzet\n"
"nyelvi kódját!"

#. create sound settings box
#: ../src/optbox.c:410
msgid "Sound on the remote Computer"
msgstr "Hang a távoli gépen"

#: ../src/optbox.c:441
msgid ""
"Please select which sound output you\n"
"would like to use."
msgstr ""
"Válassza ki, hogy melyik hangkimenetet\n"
"szeretné használni!"

#. create program settings box
#: ../src/optbox.c:464
msgid "Launch Program"
msgstr "Programindítás"

#: ../src/optbox.c:495
msgid ""
"Select a program which should start\n"
"after connection."
msgstr ""
"Válassza ki a programot, ami a\n"
"kapcsolódás után elindul!"

#. create program settings box
#: ../src/optbox.c:525
msgid "Options"
msgstr "Opciók"

#: ../src/optbox.c:556
msgid "Tune your remote desktop connections."
msgstr "A távoli kapcsolat optimalizálása."

#. create frame Disk
#: ../src/optbox.c:603
msgid "Disk"
msgstr "Diszk"

#: ../src/optbox.c:624
msgid "Select path to share on remote server."
msgstr ""
"Válassza ki a könyvtárat,\n"
"amit a távoli gépen is el akar érni!"

#: ../src/optbox.c:638
msgid "Share name:"
msgstr "Megosztás neve:"

#: ../src/optbox.c:652
msgid "Local full path of share:"
msgstr "Teljes elérési útvonal:"

#: ../src/optbox.c:672
msgid "Browse..."
msgstr "Keresés..."

#: ../src/optbox.c:678
msgid "Add"
msgstr "Felvesz"

