#: ../src/options.c:214
msgid "No sound output"
msgstr "Nincs hangkimenet"

#: ../src/options.c:215
msgid "Play on this host"
msgstr "Ezen a gépen"

#: ../src/rdpparse.c:53 ../src/rdpparse.c:81
msgid "Please select a valid rdp file!"
msgstr "Válasszon egy érvényes RDP fájlt!"

#: ../src/rdpparse.c:95
msgid "Really override the already existing file?"
msgstr "Valóban felülírjuk a létező fájlt?"

#: ../src/rdpparse.c:111
#, c-format
msgid "Unable to save rdp-file: %s"
msgstr "Nem sikerült az RDP fájlt menteni: %s"

#: ../src/rdpparse.c:187 ../src/rdpparse.c:196
#, c-format
msgid "Unable to open rdp-file: %s"
msgstr "Nem sikerült az RDP fájlt megnyitni: %s"

#: ../src/run.c:107
msgid "No hostname for ssh tunneling available"
msgstr "Nincs megadva gépnév az SSH-alagúthoz!"

#: ../src/sshbox.c:41
msgid "SSH-Tunnel settings"
msgstr "Az SSH-alagút beállításai"

#: ../src/sshbox.c:44
