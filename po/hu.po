msgstr "Felhasználónév:"

#: ../src/optbox.c:744
msgid "Password:"
msgstr "Jelszó:"

#: ../src/optbox.c:768
msgid "Domain:"
msgstr "Domain:"

#: ../src/optbox.c:782
msgid "Save my password"
msgstr "Jelszó mentése"

#: ../src/optbox.c:841
msgid "Less"
msgstr "kisebb"

#: ../src/optbox.c:857
msgid "More"
msgstr "nagyobb"

#: ../src/optbox.c:863
msgid "??? pixels"
msgstr "Hány pixel?"

#: ../src/optbox.c:936
msgid "Keyboard"
msgstr "Billentyűzet"

#. insert checkbox
